<?xml version="1.0" encoding="UTF-8"?>

<layout>
  <property>
    <name>倒品字布局器</name>
    <portNumber>3</portNumber>
    <version>V1.0 by Jon.King</version>
    <description><![CDATA[田字布局器。]]></description>
  </property>
  <script><![CDATA[]]></script>
  <style><![CDATA[]]></style>
  <prototypeStyle><![CDATA[]]></prototypeStyle>
  <html><![CDATA[<table id="${id}" border="0" width="#{tableWidth}" cellspacing="0" cellpadding="0">
	<tr>
                <td width="#{column1Width}">${port0}</td>
                <td width="#{column2Width}">${port1}</td>
	</tr>
	<tr>
                <td colspan="2">${port2}</td>
	</tr>
</table>
		]]></html>
  <events/>
