<?xml version="1.0" encoding="utf-8"?>
<Response>
  <ConfigParams>
    <xform>
      <declare>
        <column name="height" caption="单元格高度" mode="string"/>
        <column name="img" caption="背景图片" mode="string"/>
        <column name="color" caption="字体颜色" mode="string"/>
      </declare>
      <layout>
        <TR>
          <TD width="50"><label binding="height"/></TD>
          <TD><input binding="height" type="text"/></TD>
        </TR>
        <TR>
          <TD width="50"><label binding="img"/></TD>
          <TD><input binding="img" type="text"/></TD>
        </TR>
        <TR>
          <TD width="50"><label binding="color"/></TD>
          <TD><input binding="color" type="text"/></TD>
        </TR>
      </layout>
      <data></data>
    </xform>
  </ConfigParams>
</Response>