<?xml version="1.0" encoding="UTF-8"?>
<Response>
	<SearchLog>
		<xform>
			<declare prefix="userQueryCon">
				<column name="startDate" caption="起始日期" mode="date" pattern="yyyy/MM/dd"/>
				<column name="endDate" caption="截止日期" mode="date" pattern="yyyy/MM/dd"/>
				<column name="target" caption="操作对象" mode="string"/>
				<column name="type" caption="操作类型" mode="string"/>
			</declare>
			<layout>
				<TR>
					<TD width="50"><label binding="startDate"/></TD>
					<TD><input binding="startDate" type="text"/></TD>
				</TR>
				<TR>
					<TD width="50"><label binding="endDate"/></TD>
					<TD><input binding="endDate" type="text"/></TD>
				</TR>
				<TR>
					<TD width="50"><label binding="target"/></TD>
					<TD><input binding="target" type="text"/></TD>
				</TR>
				<TR>
					<TD width="50"><label binding="type"/></TD>
					<TD><input binding="type" type="text"/></TD>
				</TR>
			</layout>
			<data>
                <row/>
			</data>
		</xform>
	</SearchLog>
</Response>