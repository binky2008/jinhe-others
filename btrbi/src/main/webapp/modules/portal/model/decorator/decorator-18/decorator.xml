<?xml version="1.0" encoding="UTF-8"?>

<decorator>
  <property>
    <name>首页图片新闻</name>
    <version/>
    <description><![CDATA[]]></description>
  </property>
  <script><![CDATA[]]></script>
  <style><![CDATA[#${id} .tpxw_title {
    FONT-WEIGHT: bold; FONT-SIZE: 14px; PADDING-LEFT: 12px; COLOR: #ffffff; HEIGHT: 27px;
}]]></style>
  <prototypeStyle><![CDATA[]]></prototypeStyle>
  <html><![CDATA[<TABLE class="border_tpxw" cellSpacing=0 cellPadding=0 width=193 border=0 id="${id}">
  <TR>
    <TD class="tpxw_title" background="${basepath}tpxw_bg.gif">#{title}</TD>
  </TR>
  <TR>
    <TD height="#{height}" valign=top align=center bgColor=#ebebed>${content}</TD>
  </TR>
</TABLE>]]></html>
  <events/>
  <parameters>
    <param name="title" defaultValue="图片新闻"/>
    <param name="height" defaultValue=""/>
  </parameters>
</decorator>
